{
  "name": "esnyc-lesson-search-react",
  "version": "1.0.0",
  "type": "module",
  "description": "ESNYC Lesson Library Search Interface - A searchable database of 831 tagged Edible Schoolyard NYC lesson plans",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "import-data": "node scripts/import-data.js",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "test:lighthouse": "npm run build && lhci autorun",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/"
  },
  "keywords": [
    "education",
    "lesson-plans",
    "search",
    "edible-schoolyard",
    "teaching-resources"
  ],
  "author": "ESNYC",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "compression": "^1.7.4",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.82.0",
    "@types/node": "^24.0.12",
    "autoprefixer": "^10.4.21",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
